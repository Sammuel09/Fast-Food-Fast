{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "fast-food-fast is a food delivery app",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text cross-env NODE_ENV=test mocha --require babel-register ./server/test/test.js ./server/test/userController.js --exit",
    "createTables": "cross-env NODE_ENV=test babel-node -- server/models/createTables.js",
    "start": "nodemon bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "production": "npm run build && node bin/production",
    "test-coverage": "set NODE_ENV=test && babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --exit server/test/test.js",
    "coveralls": "npm run test-coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sammuel09/Fast-Food-Fast.git"
  },
  "keywords": [
    "fast-food"
  ],
  "author": "Samuel Adeniran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sammuel09/Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/Sammuel09/Fast-Food-Fast#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.18.4",
    "request": "^2.88.0"
  },
  "dependencies": {
    "babel-istanbul": "^0.12.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "pg": "^7.4.3"
  },
  "engines": {
    "node": "10.8.0"
  }
}
